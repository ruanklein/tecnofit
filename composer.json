{
    "name": "tecnofit/movement-ranking-api",
    "description": "API RESTful para ranking de movimentos",
    "type": "project",
    "require": {
        "php": ">=8.4",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tecnofit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tecnofit\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage/"
    }
}